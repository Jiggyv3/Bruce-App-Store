{
  "compress": {
    "passes": 5,
    "drop_console": true,
    "drop_debugger": true,
    "pure_funcs": ["console.log", "console.info", "console.debug", "assert"],
    "hoist_funs": true,
    "hoist_vars": true,
    "inline": true,
    "collapse_vars": true,
    "reduce_vars": true,
    "sequences": true,
    "booleans": true,
    "loops": true,
    "unused": true,
    "if_return": true,
    "join_vars": true,
    "keep_fargs": true
  },
  "mangle": {
    "toplevel": true
  },
  "output": {
    "comments": false,
    "ascii_only": true,
    "beautify": false
  }
}
